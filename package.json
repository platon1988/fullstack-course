{
  "name": "crm",
  "version": "1.0.0",
  "description": "fullstack app with angular and express",
	"main": "index.js",
	"engines": {
		"node": "11.5.0",
		"npm": "6.4.1"
	},
  "scripts": {
    "start": "node index",
    "server": "nodemon index",
    "client-install": "npm install --prefix client",
    "client": "npm run start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\" ",
		"heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run build --prefix client"
  },
  "keywords": [
    "express",
    "angular"
  ],
  "author": "Igor Zaytsev",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.29.2",
    "mongoose": "^5.4.9",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9"
  }
}
